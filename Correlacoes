pacotes
require(DescTools)
require(vegan)
require(rcompanion)

dados=read.table(file.choose(), header=T);dados
dados1=read.table(pipe("pbpaste"), sep="\t", header=T);dados1
names(dados)
dados1
cor.test(dados$n_crosses,dados$n_roadkills)
cor.test(dados1$SUM_Didalb,dados$n_roadkills)

##phi
phi( dados$cross,
     dados$roadkill,
     ci = TRUE,
     conf = 0.95,
     type = "perc",
     R = 1000)
